<div>
  <% @posts.each do |post| %>
  <div>
    <%= link_to post.tweet, team_post_path(post.team, post) %>
    <%= post.planned_time %>
    <%= link_to 'edit', edit_team_post_path(post.team, post) %> |
    <%= link_to 'delete', team_post_path(post.team, post), method: :delete %>
  </div>
  <hr>
  <% end %>
  <p>
    <%= link_to 'New Post!', new_team_post_path, class: "button" %>
  </p>
</div>

